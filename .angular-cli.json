{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "fy-project"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "./assets/plugins/bootstrap/css/bootstrap.css",
        "./assets/plugins/node-waves/waves.css",
        "./assets/plugins/animate-css/animate.css",
        "./assets/css/themes/all-themes.css",
        "./assets/css/style.css",
       "./assets/plugins/sweetalert/sweetalert.css",
       "./assets/plugins/dropzone/dropzone.css",
       "./assets/plugins/multi-select/css/multi-select.css",
       "./assets/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.css",
       "./assets/plugins/jquery-spinner/css/bootstrap-spinner.css",
       "./assets/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css",
       "./assets/plugins/bootstrap-select/css/bootstrap-select.css",
       "./assets/plugins/nouislider/nouislider.min.css"
      ],
      "scripts": [
        "./assets/plugins/jquery/jquery.min.js",
        "./assets/plugins/bootstrap/js/bootstrap.js",
        "./assets/plugins/bootstrap-select/js/bootstrap-select.js",
        "./assets/plugins/jquery-slimscroll/jquery.slimscroll.js",
        "./assets/plugins/node-waves/waves.js",
        "./assets/plugins/jquery-validation/jquery.validate.js",
        "./assets/js/pages/examples/sign-in.js",
        "./assets/js/admin.js",
        "./assets/js/demo.js",
        "./assets/js/pages/ui/dialogs.js",
        "./assets/plugins/dropzone/dropzone.js",
        "./assets/js/pages/forms/advanced-form-elements.js",
        "./assets/plugins/multi-select/js/jquery.multi-select.js",
        "./assets/js/pages/ui/modals.js",
        "./assets/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.js",
        "./assets/plugins/jquery-inputmask/jquery.inputmask.bundle.js",
        "./assets/plugins/jquery-spinner/js/jquery.spinner.js",
        "./assets/plugins/bootstrap-tagsinput/bootstrap-tagsinput.js",
        "./assets/plugins/nouislider/nouislider.js",
       "./assets/js/pages/examples/sign-in.js",
       "./assets/plugins/chartjs/Chart.bundle.js",
       "./assets/js/pages/charts/chartjs.js"
       


       



      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
